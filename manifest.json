{
  "name": "Unmeet",
  "manifest_version": 3,
  "version": "0.1",
  "description": "Stops you from having an awkward one on one",
  "permissions": [
    "activeTab",
    "scripting"
  ],
  "background": {
    "service_worker": "popup.js"
  },
  "icons": { 
    "16": "x-16x16.png",
    "32": "x-32x32.png",
   "128": "X.png" 
  },
  "action": {},
  "web_accessible_resources": [{
      "resources": ["*"],
      "matches": ["https://*.google.com/*"]
    }],
    "host_permissions": [
      "https://meet.google.com/*"
  ]
}